<launch>
  <!-- <node name="image_transport_1" pkg="image_transport" type="republish" args="raw in:=/camera/color/image_raw compressed out:=/camera/color/image_raw" />
  <node name="image_transport_2" pkg="image_transport" type="republish" args="raw in:=/camera/aligned_depth_to_color/image_raw compressedDepth out:=/camera/aligned_depth_to_color/image_raw" /> -->

  <!-- <node pkg="tf2_ros" type="static_transform_publisher" name="static_tf_cam_to_camlink" args="0 0 0 0 0 0 camera_link camera" /> -->

  <arg name="project" default="groundfusion2"/>
  <param name="use_sim_time" value="true" />
  <arg name="rviz" default="false" />
  <arg name="config_file" default="$(find groundfusion2)/config/cozum.yaml" />
  <param name="config_file" value="$(arg config_file)" />

  <!-- subscribe topic from bags -->
  <!-- <node pkg="$(arg project)"  type="$(arg project)_eskf"  name="$(arg project)_eskf"  output="screen" launch-prefix="xterm -e gdb"/> -->
  <node pkg="$(arg project)" type="$(arg project)_eskf" name="$(arg project)_eskf" output="screen" required="true" args="--config_yaml $(arg config_file)"/>
  
  <!-- rosbag_io  -->
  <!-- <node pkg="$(arg project)"  type="$(arg project)_eskf_rosbag"  name="$(arg project)_eskf_rosbag"  output="screen" launch-prefix="gdb -ex run \-\-args"/> -->
  <!-- <node pkg="$(arg project)" type="$(arg project)_eskf_rosbag" name="$(arg project)_eskf_rosbag" output="screen" required="true"/> -->

  <node pkg="vins" type="vins_node" name="vins_node" output="screen" args="$(find groundfusion2)/../config/realsense/cozum.yaml"/>

  <group if="$(arg rviz)">
    <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find groundfusion2)/launch/rviz_custom.rviz" />
  </group>

  <include file="$(find im_mesh)/launch/mapping_cozum.launch">
    <arg name="rviz" value="false" />  <!-- You can modify this value depending on your needs -->
  </include>

<!-- <node pkg="tf" type="static_transform_publisher" name="word2map_tf"  args="0 0 0 0 0 0 /world /map 10" />
<node pkg="tf" type="static_transform_publisher" name="bask_to_laser"  args="0 0 0 0 0 0 /base_link /rslidar 10" /> -->

</launch>
